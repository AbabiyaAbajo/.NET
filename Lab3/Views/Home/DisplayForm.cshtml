@model Lab3.Models.LoginInformationModel
@{
    Layout = "_Layout";
    ViewData["Title"] = "Display Form";
}

<h3>Display Form Data</h3>
<table>
    <tr>
        <td>
           First Name:
        </td>
        <td>
            @Model.fname
        </td>
    </tr>
    <tr>
        <td>
            Last Name:
        </td>
        <td>
            @Model.lname
        </td>
    </tr>
    <tr>
        <td>
            Age:
        </td>
        <td>
            @Model.age
        </td>
    </tr>
    <tr>
        <td>
            Email Address:
        </td>
        <td>
            @Model.emailadd
        </td>
    </tr>
    <tr>
        <td>
            Date of Birth:
        </td>
        <td>
            @Model.dob
        </td>
    </tr>
    <tr>
        <td>
            Password:
        </td>
        <td>
            @Model.Password
        </td>
    </tr>
    <tr>
        <td>
            Description:
        </td>
        <td>
            @Model.description
        </td>
    </tr>
</table>
<br />
<br />
<br />
NOTE: This model data will not persist. This data would normally be stored in a data base somewhere and then pulled again when needed.
